programa {
  funcao inicio() {
    escreva("Digite seu nome: \n")
    cadeia nome
    inteiro idade
    real altura
    leia(nome)
    escreva("Digite sua idade: \n")
    leia(idade)
    escreva("Digite sua altura separada por . (ponto) ao invés de , (virgula): \n")
    leia(altura)
    escreva("Seu nome é: ", nome, "\n")
    escreva("Sua idade é: ", idade, "\n")
    escreva("Sua altura é: ", altura)
  }
}
